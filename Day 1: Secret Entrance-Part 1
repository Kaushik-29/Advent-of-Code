#include <stdio.h>

int main() {
    int current = 50;
    int countZero = 0;
    char Di;
    int num;

    while (scanf(" %c%d", &Di, &num) == 2) {

        if (Di == 'L') {
            current = current - num;
        } else if (Di == 'R') {
            current = current + num;
        }

        current = (current % 100 + 100) % 100;

        if (current == 0) {
            countZero++;
        }
    }

    printf("%d\n", countZero);
    return 0;
}
