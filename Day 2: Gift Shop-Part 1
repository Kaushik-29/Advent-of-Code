#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <ctype.h>


int is_repeated_pattern(long long n) {
    char s[32];
    sprintf(s, "%lld", n);
    int len = strlen(s);

    if (len % 2 != 0) return 0;             
    int half = len / 2;

    for (int i = 0; i < half; i++)
        if (s[i] != s[i + half])
            return 0;

    return 1;
}

int main() {
    char input[5000];
    fgets(input, sizeof(input), stdin);

    long long total = 0;

    char *range = strtok(input, ",");
    while (range) {
        long long a, b;
        sscanf(range, "%lld-%lld", &a, &b);

        for (long long x = a; x <= b; x++) {
            if (is_repeated_pattern(x)) {
                total += x;
            }
        }

        range = strtok(NULL, ",");
    }

    printf("%lld\n", total);
    return 0;
}
