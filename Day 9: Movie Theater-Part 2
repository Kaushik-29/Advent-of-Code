#include <stdio.h>
#include <stdlib.h>

typedef struct { long long x, y; } P;

int inside(P *a, int n, long long x, long long y) {
    int c = 0;
    for (int i = 0, j = n - 1; i < n; j = i++) {
        long long xi = a[i].x, yi = a[i].y;
        long long xj = a[j].x, yj = a[j].y;
        int b1 = yi > y, b2 = yj > y;
        if (b1 != b2) {
            double xx = (double)(xj - xi) * (y - yi) / (double)(yj - yi) + xi;
            if (x < xx) c = !c;
        }
    }
    return c;
}

int edge_crosses_rect(P *a, int n, long long xmin, long long xmax, long long ymin, long long ymax) {
    for (int i = 0, j = n - 1; i < n; j = i++) {
        long long xi = a[j].x, yi = a[j].y;
        long long xj = a[i].x, yj = a[i].y;
        if (xi == xj) {
            long long x0 = xi;
            long long y1 = yi < yj ? yi : yj;
            long long y2 = yi < yj ? yj : yi;
            if (x0 > xmin && x0 < xmax) {
                long long low = y1, high = y2;
                if (low < ymax && high > ymin) return 1;
            }
        } else if (yi == yj) {
            long long y0 = yi;
            long long x1 = xi < xj ? xi : xj;
            long long x2 = xi < xj ? xj : xi;
            if (y0 > ymin && y0 < ymax) {
                long long low = x1, high = x2;
                if (low < xmax && high > xmin) return 1;
            }
        }
    }
    return 0;
}

int rect_ok(P *a, int n, long long xmin, long long xmax, long long ymin, long long ymax) {
    long long cx = xmin + (xmax - xmin) / 2;
    long long cy = ymin + (ymax - ymin) / 2;
    if (!inside(a, n, cx, cy)) return 0;
    if (edge_crosses_rect(a, n, xmin, xmax, ymin, ymax)) return 0;
    return 1;
}

int main() {
    P *v = NULL;
    long long n = 0, cap = 0;
    long long x, y;
    char line[128];

    while (fgets(line, sizeof(line), stdin)) {
        if (sscanf(line, "%lld,%lld", &x, &y) != 2) continue;
        if (n + 1 > cap) {
            cap = cap ? cap * 2 : 256;
            v = realloc(v, cap * sizeof(P));
        }
        v[n].x = x;
        v[n].y = y;
        n++;
    }

    long long best = 0;

    for (long long i = 0; i < n; i++) {
        for (long long j = i + 1; j < n; j++) {
            long long xmin = v[i].x < v[j].x ? v[i].x : v[j].x;
            long long xmax = v[i].x > v[j].x ? v[i].x : v[j].x;
            long long ymin = v[i].y < v[j].y ? v[i].y : v[j].y;
            long long ymax = v[i].y > v[j].y ? v[i].y : v[j].y;

            if (!rect_ok(v, n, xmin, xmax, ymin, ymax)) continue;

            long long area = (xmax - xmin + 1) * (ymax - ymin + 1);
            if (area > best) best = area;
        }
    }

    printf("%lld\n", best);
    free(v);
    return 0;
}
