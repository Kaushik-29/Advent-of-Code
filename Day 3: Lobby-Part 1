#include <stdio.h>
#include <string.h>

#define MAX_LEN 200000  

int main() {
    char s[MAX_LEN];
    long long total = 0;

    while (fgets(s, sizeof(s), stdin)) {
        int n = strlen(s);
        if (s[n-1] == '\n') { s[n-1] = '\0'; n--; }
        if (n == 0) continue;

        int sufmax[n];  

       
        sufmax[n-1] = s[n-1] - '0';
        for (int i = n-2; i >= 0; i--) {
            int d = s[i] - '0';
            sufmax[i] = (d > sufmax[i+1]) ? d : sufmax[i+1];
        }

      
        int best = -1;
        for (int i = 0; i < n-1; i++) {
            int first = s[i] - '0';
            int second = sufmax[i+1];

            int val = first * 10 + second;
            if (val > best)
                best = val;

        
            if (best == 99)
                break;
        }

        total += best;
    }

    printf("%lld\n", total);
    return 0;
}
